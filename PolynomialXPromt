/* 
Asks the user to enter a value for x and then displays 

the value of the following polynomial 

3x^5 + 2x^4 - 5x^3 - x^2 + 7x -6
*/

#include <stdio.h>

int main(void)
{
float x,z;
printf( " Enter the value of x:");

scanf( "%f", &x);

z = 3 * x * x * x * x * x + 2 * x * x * x * x - 5 * x * x * x - x * x + 7 * x -6 ;

printf ( " The value of polynomial with the argument %f is %f " , x , z );

return 0;

}
