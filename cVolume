/*
Compute the volume of a sphere with a 10 meter radius 
using the formula v = 4/3  * pi * r^3
*/

#include <stdio.h>

#define PI 3.14159f
#define  MULTI ( 4.0f/3.0f )

int main(void) 
{
float volume , radius = 10.0f ;

volume = MULTI * PI * ( radius * radius * radius ) ;
  
  printf("The volume of a sphere with a 10-meter radius is %.2f\n", volume);    

    return 0;
}
