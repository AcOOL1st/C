/* 
write a program that asks the user to enter a dollars-and-cents amount . 
then displays the amount with 5% tax added
*/

#include <stdio.h>

int main(void)
{
float money. moneyTax;
printf ( " Enter the dollars-and-cents amount: ");
scanf ( "%f", &money);
moneyTax = money * 0.05 + money ;
printf ( " The amount of money with Tax is %f",moneyTax);

return 0;

}
